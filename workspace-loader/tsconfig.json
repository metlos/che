{
  "include": [
    "src",
    "test"
  ],
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["dom","es6"],
    "sourceMap": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noFallthroughCasesInSwitch": true,
    "downlevelIteration": true
  }
}
